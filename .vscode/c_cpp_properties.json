{
    "configurations": [
        {
            "name": "ARM Cortex-M0+ Microchip",
            "includePath": [
                "${workspaceFolder}/**"
            ],
            "defines": [
                "__SAMD21G18A__",
                "USE_CMSIS_INIT"
            ],
            "compilerPath": "/usr/local/bin/arm-none-eabi-gcc",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "macos-gcc-arm",
            "browse": {
                "path": [
                    "${workspaceFolder}"
                ],
                "limitSymbolsToIncludedHeaders": true
            },
            "compileCommands": "${workspaceFolder}/compile_commands.json"
        }
    ],
    "version": 4
}
