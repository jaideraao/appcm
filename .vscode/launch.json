{
    "version": "0.2.0",
    "configurations": [
        {
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceFolder}/bin/app.elf",
            "gdbpath": "arm-none-eabi-gdb",
            "name": "Debug Microcontroller",
            "request": "attach",
            "stopAtConnect": true,
            "stopAtEntry": true,
            "target": "extended-remote /dev/cu.usbmodem7BAE9BAB1",
            "type": "gdb",
            "valuesFormatting": "prettyPrinters",
            "autorun": [
                "monitor tpwr enable",
                "monitor swdp_scan",
                "attach 1",
                "file ./bin/app.elf",
                "load",
                "start"
            ]
        },
        {
            "name": "Embedded Debug",
            "type": "embedded-debug",
            "request": "launch",
            "serialNumber": "${command:device-manager.getSerialNumber}",
            "program": "${command:embedded-debug.getApplicationFile}",
            "cmsisPack": "${command:device-manager.getDevicePack}",
            "debugFrom": "main",
            "pdsc": "${workspaceFolder}/cmsis/samd21/Microchip.SAMD21_DFP.3.6.144.pdsc"
        }
    ]
}
